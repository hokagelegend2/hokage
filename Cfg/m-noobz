function delete(){
clear
echo -e "$CYANâ•­â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•®${NC}"
echo -e "$CYANâ”‚ ${NC}${LIME}             ${LIME}â€¢ DELETE NOOBZ â€¢                   ${NC}$CYAN â”‚ $NC"
echo -e "$CYANâ•°â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•¯${NC}"

NUMBER_OF_CLIENTS=$(grep -c -E "^### " "/etc/xray/noob")
	if [[ ${NUMBER_OF_CLIENTS} == '0' ]]; then
		echo ""
		echo "You have no existing clients!"
		exit 1
	fi

	echo ""
	echo " Select the existing client you want to remove"
	echo " Press CTRL+C to return"
	echo " ==============================="
	echo "     No  Expired   User"
	grep -E "^### " "/etc/xray/noob" | cut -d ' ' -f 2-3 | nl -s ') '
	until [[ ${CLIENT_NUMBER} -ge 1 && ${CLIENT_NUMBER} -le ${NUMBER_OF_CLIENTS} ]]; do
		if [[ ${CLIENT_NUMBER} == '1' ]]; then
			read -rp "Select one client [1]: " CLIENT_NUMBER
		else
			read -rp "Select one client [1-${NUMBER_OF_CLIENTS}]: " CLIENT_NUMBER
		fi
	done
CLIENT_NAME=$(grep -E "^### " "/etc/xray/noob" | cut -d ' ' -f 2-3 | sed -n "${CLIENT_NUMBER}"p)
user=$(grep -E "^### " "/etc/xray/noob" | cut -d ' ' -f 2 | sed -n "${CLIENT_NUMBER}"p)
exp=$(grep -E "^### " "/etc/xray/noob" | cut -d ' ' -f 3 | sed -n "${CLIENT_NUMBER}"p)
sed -i "/^### $user $exp/d" /etc/xray/noob
sed -i '/^,"'"$user"'"$/d' /etc/noobzvpns/users.json
noobzvpns --remove-user $user
expi=`date -d "$exp days" +"%Y-%m-%d"`
systemctl restart noobzvpns

clear
echo -e "$CYANâ•­â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•®${NC}"
echo -e "$CYANâ”‚ ${NC}${LIME}              ${LIME}â€¢ DELETE NOOBZ â€¢                   ${NC}$CYAN â”‚ $NC"
echo -e "$CYANâ•°â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•¯${NC}"
echo -e ""
echo -e "$LIME USERNAME $user HAS BEEN DELETE $NC"
echo -e "$LIME EXPIRED : $expi $NC"
echo -e ""
TEXT="
<code>â—†â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â—†</code>
<code>  DELETE NOOBZVPNS</code>
<code>â—†â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â—†</code>
<code>DOMAIN   :</code> <code>${domain} </code>
<code>ISP      :</code> <code>$ISP $CITY </code>
<code>USERNAME :</code> <code>$user</code>
<code>EXPIRED  :</code> <code>$expi </code>
<code>â—†â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â—†</code>
<i>Succes Delete This User...</i>
"
curl -s --max-time $TIMES -d "chat_id=$CHATIDNEW&disable_web_page_preview=1&text=$TEXT&parse_mode=html" $URL >/dev/null
cd
if [ ! -e /etc/tele ]; then
echo -ne
else
echo "$TEXT" > /etc/notiftele
bash /etc/tele
fi
echo -e "$CYANâ•­â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• â€¢ ${LIME}BY${NC}${LIME} â€¢ $CYANâ•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•®${CYAN}"
echo -e "$CYAN${NC}            ${LIME}       â€¢ JS TUNNELING â€¢          $LIME $NC"
echo -e "$CYANâ•°â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•¯${LIME}"
read -n 1 -s -r -p "Press any key to back on menu"
menu
}


function renew(){
clear
echo -e "$CYANâ•­â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•®${NC}"
echo -e "$CYANâ”‚ ${NC}${LIME}                ${LIME}â€¢ RENEW NOOBZ â€¢                  ${NC}$CYAN â”‚ $NC"
echo -e "$CYANâ•°â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•¯${LIME}"
read -p "username: " user
read -p "expired?: " exp
noobzvpns --renew-user $user --expired-user $user $exp
clear
echo -e "$CYANâ•­â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•®${NC}"
echo -e "$CYANâ”‚ ${NC}${LIME}                ${LIME}â€¢ RENEW NOOBZ â€¢                  ${NC}$CYAN â”‚ $NC"
echo -e "$CYANâ•°â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•¯${LIME}"
echo "USERNAME: $user"
echo "renew success!!"
echo -e "$CYANâ•­â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• â€¢ ${LIME}BY${NC}${LIME} â€¢ $CYAN â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•®${NC}"
echo -e "$CYAN${NC}               ${LIME}      â€¢ JS TUNNELING â€¢         $CYAN $NC"
echo -e "$CYANâ•°â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•¯${LIME}"
read -n 1 -s -r -p "Press any key to back on menu"
menu
}




function lock(){
clear
echo -e "$CYANâ•­â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•®${NC}"
echo -e "$CYANâ”‚ ${NC}${LIME}              ${LIME}â€¢ LOCK  NOOBZ â€¢                    ${NC}$CYAN â”‚ $NC"
echo -e "$CYANâ•°â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•¯${LIME}"
read -p "username: " user
noobzvpns --block-user $user
clear
echo -e "$CYANâ•­â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•®${NC}"
echo -e "$CYANâ”‚ ${NC}${LIME}              ${LIME}â€¢ LOCK  NOOBZ â€¢                    ${NC}$CYAN â”‚ $NC"
echo -e "$CYANâ•°â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•¯${LIME}"
echo "USERNAME: $user"
echo "locked success!!"
echo -e "$CYANâ•­â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• â€¢ ${LIME}BY${NC}${LIME} â€¢ $CYANâ•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•®${NC}"
echo -e "$CYAN${NC}              ${LIME}         â€¢ JS TUNNELING â€¢           $CYAN $NC"
echo -e "$CYANâ•°â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•¯${LIME}"
read -n 1 -s -r -p "Press any key to back on menu"
menu
}






function show(){

clear
echo -e "$CYANâ•­â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•®${NC}"
echo -e "$CYANâ”‚ ${NC}${LIME}               ${LIME}â€¢ MEMBER  NOOBZ â€¢                 ${NC}$CYAN â”‚ $NC"
echo -e "$CYANâ•°â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•¯${LIME}"
noobzvpns --info-all-user | awk '/^\s*\+.*-> active/ {gsub(/(issued\(yyyymmdd\)|hash_key): [0-9a-f]+/, ""); print; getline; print; getline; getline; getline; print; print "â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•"}'

echo -e "$CYANâ•­â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• â€¢ ${LIME} BY ${NC} ${CYAN} â€¢ â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•®${CYAN}"
echo -e "$CYAN${NC}              ${LIME}      â€¢ bumiayuvpn â€¢          $CYAN $LIME"
echo -e "$CYANâ•°â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•¯${LIME}"
read -n 1 -s -r -p "Press any key to back on menu"
menu

}



function unlock(){
clear
echo -e "$CYANâ•­â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•®${NC}"
echo -e "$CYANâ”‚ ${NC}${LIME}             ${LIME}â€¢ UNLOCK  NOOBZ â€¢                   ${NC}$CYAN â”‚ $NC"
echo -e "$CYANâ•°â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•¯${LIME}"
read -p "username: " user
noobzvpns --unblock-user $user
clear
echo -e "$CYANâ•­â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•®${NC}"
echo -e "$CYANâ”‚ ${NC}${LIME}             ${LIME}â€¢ UNLOCK  NOOBZ â€¢                   ${NC}$CYAN â”‚ $NC"
echo -e "$CYANâ•°â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•¯${LIME}"
echo "USERNAME: $user"
echo "unlocked success!!"
echo -e "$CYANâ•­â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• â€¢ ${LIME}BY${NC}${LIME} â€¢ $CYANâ•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•®${NC}"
echo -e "$CYAN${NC}               ${LIME}       â€¢ bumiayuvpn â€¢          $CYAN $NC"
echo -e "$CYANâ•°â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•¯${LIME}"
read -n 1 -s -r -p "Press any key to back on menu"
menu
}



function remove(){
clear
echo -e "$CYANâ•­â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•®${NC}"
echo -e "$CYANâ”‚ ${NC}${LIME}             ${LIME}â€¢ REMOVE  NOOBZ â€¢                   ${NC}$CYAN â”‚ $NC"
echo -e "$CYANâ•°â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•¯${LIME}"

read -p "Apakah Anda ingin menghapus semua user? (Y/N): " choice

if [ "$choice" == "Y" ] || [ "$choice" == "y" ]; then
    # Eksekusi perintah di sini
    echo "MENGHAPUS SEMUA USER!"
noobzvpns --remove-all-user
elif [ "$choice" == "N" ] || [ "$choice" == "n" ]; then
    echo "Membatalkan penghapusan."
menu-noobzvpns
else
    echo "Pilihan tidak valid."
fi

clear
echo -e "$CYANâ•­â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•®${NC}"
echo -e "$CYANâ”‚ ${NC}${LIME}             ${LIME}â€¢ REMOVE  NOOBZ â€¢                   ${NC}$CYAN â”‚ $NC"
echo -e "$CYANâ•°â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•¯${LIME}"
echo "USERNAME: $user"
echo "unlocked success!!"
echo -e "$CYANâ•­â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• â€¢ ${LIME}BY${NC}${LIME} â€¢ $CYANâ•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•®${CYAN}"
echo -e "$CYAN${NC}              ${LIME}             â€¢ bumiayuvpn â€¢                 $CYAN $NC"
echo -e "$CYANâ•°â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•¯${LIME}"
read -n 1 -s -r -p "Press any key to back on menu"
menu
}




clear
echo -e "$CYANâ•­â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•®${NC}"
echo -e "$CYANâ”‚ ${NC}${LIME}             ${LIME} â€¢ NOOBZ PANEL MENU â€¢            ${NC}$CYAN â”‚ $NC"
echo -e "$CYANâ•°â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•¯${NC}"
echo -e "$CYANâ•­â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•®${NC}"
echo -e "$CYANâ”‚ $NC  ${YELLOW}[${YELLOW}01${YELLOW}]${NC} ${LIME}â€¢ ${LIME}ADD AKUN${NC}      ${YELLOW}[${YELLOW}04${YELLOW}]${NC} ${LIME}â€¢ ${LIME}DELETE USER${NC}   $LIME $NC"
echo -e "$CYANâ”‚ $NC  ${YELLOW}[${YELLOW}02${YELLOW}]${NC} ${LIME}â€¢ ${LIME}SHOW AKUN${NC}     ${YELLOW}[${YELLOW}05${YELLOW}]${NC} ${LIME}â€¢ ${LIME}LOCKED USER${NC}    $LIME $NC"
echo -e "$CYANâ”‚ $NC  ${YELLOW}[${YELLOW}03${YELLOW}]${NC} ${LIME}â€¢ ${LIME}RENEW AKUN${NC}    ${YELLOW}[${YELLOW}06${YELLOW}]${NC} ${LIME}â€¢ ${LIME}UNLOCKED USER${NC}    $LIME $NC"
echo -e "$CYANâ”‚ $NC  ${YELLOW}[${YELLOW}00${YELLOW}]${NC} ${LIME}â€¢ ${LIME}GO BACK${NC}       ${YELLOW}[${YELLOW}07${YELLOW}]${NC} ${LIME}â€¢ ${LIME}REMOVE ALL USER${NC}    $LIME $NC"
echo -e "$CYANâ•°â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•¯${NC}"
echo -e "$CYANâ•­â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â• ${LIME} â€¢ ${LIME} BY ${LIME} â€¢ ${CYAN} â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•®${CYAN}"
echo -e "$CYAN${NC}               ${LIME}   â€¢ bumiayuvpn â€¢       $LIME $NC"
echo -e "$CYANâ•°â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•¯${LIME}"
echo -e ""
echo -ne " ${LIME}Select menu ${LIME}: ${LIME}"; read opt
case $opt in
01 | 1) clear ; create ;;
02 | 2) clear ; show ;;
03 | 3) clear ; renew ;;
04 | 4) clear ; delete ;;
05 | 5) clear ; lock ;;
06 | 6) clear ; unlock ;;
07 | 7) clear ; remove ;;
100) clear ; $up2u ;;
00 | 0) clear ; menu ;;
*) clear ; menu ;;
esac
